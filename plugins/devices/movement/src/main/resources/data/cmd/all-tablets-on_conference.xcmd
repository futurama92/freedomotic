<command>
    <name>Turn on all Tablets</name>
    <receiver>app.events.sensors.behavior.request.objects</receiver>
    <description>turn on all lights</description>
    <delay>0</delay>
    <timeout>0</timeout>
    <properties>
        <properties>
            <property name="object.class" value="EnvObject.ElectricDevice.Table"/>
            <property name="behavior" value="powered"/>
            <property name="value" value="true"/>
            <property name="object.zone" value="conference_room"/>
        </properties>
    </properties>
</command>