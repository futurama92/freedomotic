<command>
    <name>Turn Glucometer sensors off</name>
    <receiver>app.events.sensors.behavior.request.objects</receiver>
    <description>Turn Glucometer sensors off</description>
    <delay>0</delay>
    <timeout>0</timeout>
    <properties>
        <properties>
            <property name="object" value="Glucometer"/>
            <property name="behavior" value="takeThePills"/>
            <property name="value" value="false"/>
        </properties>
    </properties>
</command>
